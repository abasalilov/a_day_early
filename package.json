{
  "name": "ade-ui",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf public",
    "postclean": "npm-run-all --parallel prod:*",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "build": "NODE_ENV=production npm run clean",
    "prod:build-server": "webpack --config webpack.server.js ",
    "prod:build-client": "webpack --config webpack.client.js ",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "postbuild": "node build/bundle.js"
  },
